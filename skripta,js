
<script>
  $(document).ready(function() {
    $("#target1").css("color", "red");
    $("#target1").prop("disabled", true);
    $("#target4").remove();
    $("#target2").appendTo("#right-well");
    
    
    // Jonah
    
    // when adding to .css("width", "100"); the picture inside #prvi takes the right border, untill
    // you increase it by one it adds red border down, than again by 1 pixel it adds border to the right
    
    
    $("#prvi").css({"border":"1px solid red", "margin":"0px auto", "width": "102px", "margin-bottom":"10px"});
    $("#drugi").css({"border":"1px solid green", "margin":"0px auto", "width": "102px", "margin-bottom":"10px"});
     $("#treci").css({"border":"1px solid blue", "margin":"0px auto", "width": "102px", "margin-bottom":"10px"});
    
    
    // Disable
    
     $("#prvi").appendTo("#treci");
     $(".slika3").remove()
    
    // Appending
    
  
 

    
    
    
  });
</script>

<!-- Only change code above this line. -->

